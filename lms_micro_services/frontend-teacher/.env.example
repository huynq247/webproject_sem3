# Teacher Portal - Environment Variables Example
# Copy this file to .env and update with your values

# API Gateway and Services
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_AUTH_SERVICE_URL=http://localhost:8001
REACT_APP_CONTENT_SERVICE_URL=http://localhost:8002
REACT_APP_ASSIGNMENT_SERVICE_URL=http://localhost:8004

# Feature Flags
REACT_APP_DEBUG=true
REACT_APP_ENABLE_ANALYTICS=false

# UI Settings
REACT_APP_ITEMS_PER_PAGE=10
REACT_APP_TIMEOUT_MS=30000

# App Info
REACT_APP_NAME=LMS Teacher Portal
REACT_APP_VERSION=1.0.0
